
const user = {
  id: 1,
  name: "Alice",
  address: {
    city: "Chennai",
    pincode: 600001
  }
};


const { name, address: { city } } = user;
console.log(name);   
console.log(city);  


console.log(user.name);     
console.log(user["address"]["pincode"]); 


for (let key in user) {
  console.log(`${key}: ${user[key]}`);
}
